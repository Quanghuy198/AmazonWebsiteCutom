<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-wEmeIV1mKuiNpC+IOBjI7aAzPcEZeedi5yW5f2yOq55WWLwNGmvvx4Um1vskeMj0" crossorigin="anonymous">
    <link rel="stylesheet" href="~/lib/themify-icons/themify-icons.css">
    <link rel="stylesheet" href="~/css/login.css">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
</head>
<body>
    <header>
        <div class="wrap-logo">
        <img src="./img/logo-black.png" alt="" class="img-logo">
        </div>
    </header>

    <div class="container-fluid">
        @RenderBody()
    </div>

    <div class="footer footer-login">
        <div class="link-footer a-section">
            <a href="#">Conditions of use</a>
            <a href="#">Private Notice</a>
            <a href="#">Help</a>
        </div>
        <div class="footer-lable">
            <span>© 1996-2021, Amazon.com, Inc. or its affiliates</span>
        </div>
    </div>
    <script src="~/js/vadidate.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-p34f1UUtsS3wqzfto5wAAmdvj+osOnFyQFpp4Ua3gs/ZVWx6oOypYoCJhGGScy+8" crossorigin="anonymous"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
